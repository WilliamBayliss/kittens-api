<div>
    <%= form_for :kitten, url: kitten_path(@kitten.id), method: :put do |f| %>
        <%= f.label :name %>
        <%= f.text_field :name %>
        <%= f.label :age %>
        <%= f.number_field :age %>
        <%= f.label :cuteness %>
        <%= f.number_field :cuteness %>
        <%= f.label :softness %>
        <%= f.number_field :softness %>
        <%= f.submit %>
    <% end %>

</div>