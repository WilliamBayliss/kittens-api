<div class="form">
    <%= form_for :kitten, url: kittens_path do |f| %>
        <%= f.label :name %>
        <%= f.text_field :name %>
        <%= f.label :age %>
        <%= f.number_field :age %>
        <%= f.label :cuteness %>
        <%= f.number_field :cuteness %>
        <%= f.label :softness %>
        <%= f.number_field :softness %>
        <%= f.submit %>
    <% end %>
    
</div>