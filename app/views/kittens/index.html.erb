<div>
    <button><%= link_to "New Kitten", new_kitten_path %></button>
    <% @kittens.each do |kitten| %>
    <div>
        <p>Name: <%= link_to kitten.name, kitten_path(kitten) %></p>
        <p>Age: <%= kitten.age %></p>
        <p>Cuteness: <%= kitten.cuteness %></p>
        <p>Softness: <%= kitten.softness %></p>
        <br />

        <%= link_to "Delete Kitten", kitten_path(kitten), method: :delete %>
    </div>
    <br />
    <% end %>
</div>